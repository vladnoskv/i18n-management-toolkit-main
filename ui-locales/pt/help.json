        "i18ntk analyze",
        "i18ntk analyze --fix",
        "i18ntk analyze ./src/i18n"
      ]
    },
    "validate": {
      "description": "Validar estrutura e integridade dos arquivos",
      "usage": "i18ntk validate [diretório]",
      "examples": [
        "i18ntk validate",
        "i18ntk validate ./locales"
      ]
    },
    "summary": {
      "description": "Gerar relatório de resumo das traduções",
      "usage": "i18ntk summary [opções] [diretório]",
      "examples": [
        "i18ntk summary",
        "i18ntk summary --format json",
        "i18ntk summary ./locales"
      ]
    },
    "usage": {
      "description": "Analisar uso de chaves de tradução",
      "usage": "i18ntk usage [opções] [diretório]",
      "examples": [
        "i18ntk usage",
        "i18ntk usage --unused",
        "i18ntk usage ./src"
      ]
    },
    "complete": {
      "description": "Verificar completude das traduções",
      "usage": "i18ntk complete [opções] [diretório]",
      "examples": [
        "i18ntk complete",
        "i18ntk complete --missing",
        "i18ntk complete ./locales"
      ]
    },
    "sizing": {
      "description": "Analisar tamanhos de arquivos de tradução",
