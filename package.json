{
  "name": "i18n-management-toolkit",
  "version": "1.4.0",
  "description": "A comprehensive internationalization (i18n) management toolkit for JavaScript/TypeScript projects",
  "main": "i18ntk-manage.js",
  "bin": {
    "i18ntk-manage": "./i18ntk-manage.js",
    "i18ntk-init": "./i18ntk-init.js",
    "i18ntk-analyze": "./i18ntk-analyze.js",
    "i18ntk-validate": "./i18ntk-validate.js",
    "i18ntk-usage": "./i18ntk-usage.js",
    "i18ntk-complete": "./i18ntk-complete.js",
    "i18ntk-sizing": "./i18ntk-sizing.js",
    "i18ntk-summary": "./i18ntk-summary.js",
    "i18ntk-autorun": "./i18ntk-autorun.js",
    "i18ntk-debug": "./dev/debug/debugger.js"
  },
  "scripts": {
    "start": "node i18ntk-manage.js",
    "i18ntk": "node i18ntk-autorun.js",
    "i18ntk:init": "node i18ntk-init.js",
    "i18ntk:analyze": "node i18ntk-analyze.js",
    "i18ntk:validate": "node i18ntk-validate.js",
    "i18ntk:usage": "node i18ntk-usage.js",
    "i18ntk:complete": "node i18ntk-complete.js",
    "i18ntk:sizing": "node i18ntk-sizing.js",
    "i18ntk:sizing:detailed": "node i18ntk-sizing.js --detailed",
    "i18ntk:debug": "node i18ntk-manage.js --command=debug",
    "i18ntk:settings": "node i18ntk-manage.js --command=settings",
    "i18ntk:summary": "node i18ntk-summary.js",
    "i18ntk:manage": "node i18ntk-manage.js",
    "i18ntk:autorun": "node i18ntk-autorun.js",
    "workflow": "node i18ntk-autorun.js",
    "init": "node i18ntk-init.js",
    "analyze": "node i18ntk-analyze.js",
    "validate": "node i18ntk-validate.js",
    "usage": "node i18ntk-usage.js",
    "complete": "node i18ntk-complete.js",
    "sizing": "node i18ntk-sizing.js",
    "summary": "node i18ntk-summary.js",
    "full-coverage": "npm run complete && npm run analyze"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "translation",
    "management",
    "toolkit",
    "javascript",
    "typescript",
    "react",
    "vue",
    "angular",
    "cli"
  ],
  "author": {
    "name": "Vladimir Noskov",
    "email": "vladnoskv@gmail.com",
    "url": "https://github.com/vladnoskv"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladnoskv/i18n-management-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/vladnoskv/i18n-management-toolkit/issues"
  },
  "homepage": "https://github.com/vladnoskv/i18n-management-toolkit#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "*.js",
    "utils/",
    "dev/",
    "ui-locales/",
    "locales/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "dependencies": {},
  "peerDependencies": {
    "i18next": ">=20.0.0",
    "react-i18next": ">=11.0.0"
  },
  "peerDependenciesMeta": {
    "i18next": {
      "optional": false
    },
    "react-i18next": {
      "optional": true
    }
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/vladnoskv"
  }
}
